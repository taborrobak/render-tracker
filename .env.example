# Example environment variables for production deployment
# Copy this to .env and fill in your actual values

# Wasabi S3 Configuration (using boto3 SDK)
AWS_ACCESS_KEY_ID=your_wasabi_access_key_here
AWS_SECRET_ACCESS_KEY=your_wasabi_secret_key_here
WASABI_ENDPOINT_URL=https://s3.eu-west-2.wasabisys.com
WASABI_BUCKET_NAME=tabcorp-data
WASABI_PREFIX=simtest4

# Optional: Enable/disable Wasabi integration
ENABLE_WASABI_DELETION=true

# Authentication
RENDERFLOW_PASSWORD=your_secure_password_here
